{
    "name": "palmyr/webapp",
    "description": "Light web app",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Palmyr\\WebApp\\": "src/"
        }
    },
    "require": {
        "php": "~8.0",
        "palmyr/commonutils": "^0.0.1",
        "composer/composer": "^2.1"
    },
    "bin": [
        "bin/install"
    ],
    "scripts": {
        "post-install-cmd": [
            "Palmyr\\WebApp\\Install\\Script\\InstallScript::install"
        ],
        "post-update-cmd": [
            "Palmyr\\WebApp\\Install\\Script\\InstallScript::install"
        ]
    }
}
